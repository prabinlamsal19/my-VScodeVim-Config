{
  "workbench.iconTheme": "material-icon-theme",
  "git.openRepositoryInParentFolders": "never",
  "workbench.layoutControl.enabled": false,
  "editor.formatOnSave": true,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "yaml.schemas": {
    "file:///home/user/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.bitbucket.explorer.refreshInterval": 0,
  "atlascode.jira.enabled": false,
  "atlascode.bitbucket.pipelines.refreshInterval": 0,
  "terminal.integrated.env.osx": {},
  "leetcode.workspaceFolder": "/Users/ebpearls/Documents/go 74",
  "redhat.telemetry.enabled": false,
  "RainbowBrackets.depreciation-notice": false,
  "window.zoomLevel": -2,
  "keyboard.dispatch": "keyCode",
  "prettier.printWidth": 130,
  "dart.lineLength": 130,
  "workbench.editor.limit.enabled": true,
  "background.fullscreen": {
    "images": ["https://ibb.co/CsQtzgg"], 
    "opacity": 0.91, // 0.85 ~ 0.95 recommended
    "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
    "position": "center", // alias to `background-position`, default `center`
    "interval": 0 // seconds of interval for carousel, default `0` to disabled.
  },

  "workbench.editor.limit.value": 7,
  "[dart]": {
    "editor.rulers": [130],
    "editor.wordWrapColumn": 130
  },

  //vimconfig
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // NAVIGATION
    // switch b/w buffers
    { "before": ["<S-h>"], "commands": [":bprevious"] },
    { "before": ["<S-l>"], "commands": [":bnext"] },

    // splits
    { "before": ["leader", "v"], "commands": [":vsplit"] },
    { "before": ["leader", "s"], "commands": [":split"] },

    // panes
    {
      "before": ["leader", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["leader", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["leader", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["leader", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    // NICE TO HAVE
    { "before": ["leader", "w"], "commands": [":w!"] },
    { "before": ["leader", "q"], "commands": [":q!"] },
    { "before": ["leader", "x"], "commands": [":x!"] },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.action.goto"]
    },
    {
      "before": ["<leader>", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    { "before": ["leader", "f"], "commands": ["workbench.action.findInFiles"] },
    // { "before": ["leader", "p"], "commands": ["editor.action.formatDocument"] },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    }
  ],
  "vim.visualModeKeyBindings": [
    // Stay in visual mode while indenting
    { "before": ["<"], "commands": ["editor.action.outdentLines"] },
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    // Move selected lines while staying in visual mode
    { "before": ["J"], "commands": ["editor.action.moveLinesDownAction"] },
    { "before": ["K"], "commands": ["editor.action.moveLinesUpAction"] },
    // toggle comment selection
    { "before": ["leader", "c"], "commands": ["editor.action.commentLine"] }
  ],
  "leetcode-problem-rating.workspaceFolder": "/home/user/.lcpr",
  "leetcode.endpoint": "leetcode",
  "github.copilot.editor.enableAutoCompletions": true,
  "animations.Install-Method": "Apc Customize UI++",
  "glassit.alpha": 255
}
